<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cool.lytton.doc.mapper.UserMapper">
    <select id="getAll" resultType="cool.lytton.doc.model.User">
        select * from tb_user
    </select>

    <select id="getById" resultType="cool.lytton.doc.model.User">
        select * from tb_user where id=#{id}
    </select>
</mapper>
